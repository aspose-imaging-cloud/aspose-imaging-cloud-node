{
    "compilerOptions": {
        "module": "commonjs",
        "noImplicitAny": false,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "suppressImplicitAnyIndexErrors": true,
        "target": "ES5",
        "moduleResolution": "node",
        "removeComments": false,
        "sourceMap": true,
        "noLib": false,
        "declaration": true,
        "lib": ["es2015", "dom"],
        "outDir": "dist",
        "rootDir": "./"
    },
    "include": [
        "lib"
    ],
    "exclude": [
        "node_modules",
        "exclude"
    ]
}
